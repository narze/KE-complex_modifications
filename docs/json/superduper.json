{
  "title": "(S)uper(D)uper Mode",
  "rules": [
    {
      "description": "S+D -> (S)uper(D)uper Mode",
      "manipulators": [
        {
          "from": {
            "simultaneous": [
              {
                "key_code": "s"
              },
              {
                "key_code": "d"
              }
            ],
            "simultaneous_options": {
              "detect_key_down_uninterruptedly": true,
              "key_down_order": "insensitive",
              "key_up_order": "insensitive",
              "key_up_when": "any",
              "to_after_key_up": [
                {
                  "set_variable": {
                    "name": "superduper-mode",
                    "value": 0
                  }
                }
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "superduper-mode",
                "value": 1
              }
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "(S)uper(D)uper + H -> Left",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "conditions": [
            {
              "name": "superduper-mode",
              "value": 1,
              "type": "variable_if"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "(S)uper(D)uper + J -> Down",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "conditions": [
            {
              "name": "superduper-mode",
              "value": 1,
              "type": "variable_if"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "(S)uper(D)uper + K -> Up",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "conditions": [
            {
              "name": "superduper-mode",
              "value": 1,
              "type": "variable_if"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "(S)uper(D)uper + L -> Right",
      "manipulators": [
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "conditions": [
            {
              "name": "superduper-mode",
              "value": 1,
              "type": "variable_if"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "(S)uper(D)uper + A -> Option",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
